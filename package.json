{
  "name": "ali90h.github.io",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "fetch": "node ./src/scripts/fetch-github.mjs",
    "build": "npm run fetch && vite build",
    "preview": "vite preview --outDir dist",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "size": "size-limit"
  },
  "size-limit": [
    {
      "path": "dist/assets/index-*.js",
      "limit": "35 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-app": "^11.2.0",
    "@vitest/coverage-v8": "^1.6.0",
    "jsdom": "^26.1.0",
    "size-limit": "^11.2.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18"
  }
}
