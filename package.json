{
  "name": "ali90h-github-io",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "fetch:data": "node scripts/fetch-github.mjs",
    "build": "npm run fetch:data && tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run --coverage",
    "test:watch": "vitest",
    "size": "npm run build && size-limit"
  },
  "size-limit": [
    {
      "path": "dist/assets/app-*.js",
      "limit": "35 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-app": "^11.2.0",
    "@vitest/coverage-v8": "^3.2.4",
    "jsdom": "^26.1.0",
    "size-limit": "^11.2.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  }
}
